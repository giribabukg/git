<div style="margin:1em">
  <div class="fl" style="margin-right:1em;">
    <div class="box p16" style="min-width:100px; min-height:100px;">
      <img src="index.php?act=utl-pixelboxx.thumb&amp;doi={doi}" alt="thumbnail {doi}"/>
    </div>
    <br />
    <!-- a class="nav ac" onclick="Flow.Std.ajxFil('{src}','{jid}','{sub}','{age}')">&lt; Go Back</a> -->
    <a class="nav ac" onclick="Flow.Std.ajxUpd({&quot;act&quot;:&quot;job-art-fil.get&quot;,&quot;src&quot;:&quot;{src}&quot;,&quot;jid&quot;:&quot;{jid}&quot;,&quot;sub&quot;:&quot;pixelboxx&quot;,&quot;div&quot;:&quot;{div}&quot;,&quot;age&quot;:&quot;job&quot;,&quot;loading_screen&quot;:true})">&lt; Go Back</a>
    <br />
    <br />

  </div>
  
  <div class="fl" style="margin-right:1em;">
    <table class="tbl" cellpadding="4">
      <thead>
        <tr>
          <td class="th2">Field</td>
          <td class="th2" style="min-width:400px">Value</td>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td class="td2">Name</td>
          <td class="td1">{val.name}</td>
        </tr>
        <tr>
          <td class="td2">Filename</td>
          <td class="td1">{val.filename}</td>
        </tr>
        <tr>
          <td class="td2">Size</td>
          <td class="td1">{val.xsize} x {val.ysize}</td>
        </tr>
        <tr>
          <td class="td2">Filesize</td>
          <td class="td1">{val.filesize}</td>
        </tr>
        <tr>
          <td class="td2">Filetype</td>
          <td class="td1">{val.filetype}</td>
        </tr>
        <tr>
          <td class="td2">Application</td>
          <td class="td1">{val.application}</td>
        </tr>
        <tr>
          <td class="td2">File Version</td>
          <td class="td1">{val.versionnumber}</td>
        </tr>
        <tr>
          <td class="td2">Import Date</td>
          <td class="td1">{val.importdate}</td>
        </tr>
        <tr>
          <td class="td2">Last Modified</td>
          <td class="td1">{val.objmodlast}</td>
        </tr>
        <tr>
          <td class="td2">Uploaded By</td>
          <td class="td1">{val.byline}</td>
        </tr>
        <tr>
          <td class="td2">Category</td>
          <td class="td1">{val.category}</td>
        </tr>
        <tr>
          <td class="td2">Keywords</td>
          <td class="td1">{val.keywords}</td>
        </tr>
        <tr>
          <td class="th2 cp" colspan="2" onclick="Flow.pixelboxx.toggleDetails('{div}')">All Fields</td>
        </tr>
        
        
        
        {val.all}
      </tbody>
    </table>
  </div>
  <div class="fl" style="margin-right:1em; min-width:150px">
    <div class="box">{menu}</div>
  </div>
  
  <div style="clear:both"></div>
</div>
<br /><br />